/* components/proitem.wxss */
.img-box{
    position: relative;
    height: 380rpx;
    width: 100%;
}
.img{
    position: relative;
    height: 380rpx;
}
.img image{
    width: 100%;
    height: 100%;
}
.img-mask{
    background: #000;
    opacity: 0.48;
    top: 0;
    left: 0;
    position: absolute;
    width: 100%;
    height: 100%;
    z-index: 5;
}
.desc{
    top: 0;
    left: 0;
    position: absolute;
    /* background: #000;
    opacity: 0.48; */
    width: 100%;
    height: 100%;
    z-index: 5;
}
.desc view{
    text-align: center;
    color: #fff;
    margin-top: 14rpx;
    font-size: 34rpx;
    font-family: 'PingFangSC-Regular';
}
.desc .title{
    margin-top: 100rpx;
    font-family: 'PingFangSC-Medium';
    font-size: 48rpx;
}
.desc .intro{
    margin-top: 10rpx;
}
.desc .link{
    margin-top: 60rpx;
}
.status{
    position: absolute;
    top: 0;
    left: 0;
    z-index: 9;
}
.status-img{
    width: 120rpx;
    height: 48rpx;
}
.status .status-ing{
    background: url('data:image/png;base64,');
    background-size: cover;
}
.status-ed{
    background: url('data:image/png;base64,');
    background-size: cover;
}